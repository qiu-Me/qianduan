#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/VS code代码/MyAPI/node_modules/.store/json-server@1.0.0-alpha.23/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/VS code代码/MyAPI/node_modules/.store/json-server@1.0.0-alpha.23/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/json-server@1.0.0-alpha.23/node_modules/json-server/lib/bin.js" "$@"
else
  exec node  "$basedir/../.store/json-server@1.0.0-alpha.23/node_modules/json-server/lib/bin.js" "$@"
fi
